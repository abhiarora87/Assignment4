@{
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<Assignment4.Models.Sales_Model.Salesperson>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Vehicle Inventory Systems</title>
    <link href="css/main.css" type="text/css" rel="stylesheet">
</head>

<body>

    <div class="content-wrap">
        <div class="container">
            <h1>Welcome! to the Dunder Mifflin Inc</h1>
            <div>
                <marquee style="color:red">'You miss 100 percent of the chances you don't take.' </marquee>

            </div>
            <h2></h2>
            <img src="~/img/car-banner.png" alt="Vehicle inventory" width="100%" height="400">

            <br>
            <p style="max-width:800px">
                Our company website is committed to providing sales data to our partners in a quick and easy to understand manner.
            </p>

        </div>
    </div>
    <div>
        <h5>Description</h5>
        <p style="max-width:800px">
            Thank you for visiting our website. Our company provides hosts
            of services to our customers with the latest real time information
            of the avialable vehicles inventory with both new and used/ certified pre owned options
            avialable with attractive financing options.
            <br />
            Also we do provide the various past car sales made by our highly professional salesman
            alongwith the contact details.
        </p>
    </div>
    
    <div>
        <h5>Our Team</h5>
        Meet our dedicated team of sales people who work diligently 
        @foreach (var item in Model)
        {
            <ul>
                <li>
                    @item.name || @item.email
                </li>
            </ul>
        }
        </div>
</body>

</html>






